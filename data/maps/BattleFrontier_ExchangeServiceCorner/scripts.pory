script BattleFrontier_ExchangeServiceCorner_EventScript_HyperTraining {
    lock
    faceplayer
    msgbox(format("Hello! I can hypertraining your POKéMON in exchange for BOTTLE CAPS. Do you want to?"), MSGBOX_YESNO)
    if(var(VAR_RESULT) == YES){
        message(format("Which kind of BOTTLE CAP do you want to use?"))
        multichoice(25, 8, MULTI_HYPERTRAINING, 0)
        closemessage
        if(var(VAR_RESULT) == 0){
            checkitem(ITEM_BOTTLE_CAP)
            if(var(VAR_RESULT) == YES){
                msgbox(format("Choose a POKéMON to hypertrain."))
                special(ChoosePartyMon)
                waitstate
                message(format("Which stat do you want to hypertrain?"))
                multichoice(25, 0, MULTI_HYPERTRAINING_STAT, 0)
                closemessage
                if(var(VAR_RESULT) == 0){
                    canhypertrain(0, VAR_0x8004)
                    if(var(VAR_RESULT) == YES) {
                        removeitem(ITEM_BOTTLE_CAP)
                        hypertrain(STAT_HP, VAR_0x8004)
                        msgbox(format("Your POKéMON has been hypertrained in HP!"))
                    } else {
                        msgbox(format("HP stat of your POKéMON can no longer be trained."))
                    }
                } elif(var(VAR_RESULT) == 1){
                    canhypertrain(1, VAR_0x8004)
                    if(var(VAR_RESULT) == YES) {
                        removeitem(ITEM_BOTTLE_CAP)
                        hypertrain(STAT_ATK, VAR_0x8004)
                        msgbox(format("Your POKéMON has been hypertrained in ATTACK!"))
                    } else {
                        msgbox(format("ATTACK stat of your POKéMON can no longer be trained."))
                    }
                } elif(var(VAR_RESULT) == 2){
                    canhypertrain(2, VAR_0x8004)
                    if(var(VAR_RESULT) == YES) {
                        removeitem(ITEM_BOTTLE_CAP)
                        hypertrain(STAT_DEF, VAR_0x8004)
                        msgbox(format("Your POKéMON has been hypertrained in DEFENSE!"))
                    } else {
                        msgbox(format("DEFENSE stat of your POKéMON can no longer be trained."))
                    }
                } elif(var(VAR_RESULT) == 3){
                    canhypertrain(4, VAR_0x8004)
                    if(var(VAR_RESULT) == YES) {
                        removeitem(ITEM_BOTTLE_CAP)
                        hypertrain(STAT_SPATK, VAR_0x8004)
                        msgbox(format("Your POKéMON has been hypertrained in SP. ATK!"))
                    } else {
                        msgbox(format("SP. ATK stat of your POKéMON can no longer be trained."))
                    }
                } elif(var(VAR_RESULT) == 4){
                    canhypertrain(5, VAR_0x8004)
                    if(var(VAR_RESULT) == YES) {
                        removeitem(ITEM_BOTTLE_CAP)
                        hypertrain(STAT_SPDEF, VAR_0x8004)
                        msgbox(format("Your POKéMON has been hypertrained in SP. DEF!"))
                    } else {
                        msgbox(format("SP. DEF stat of your POKéMON can no longer be trained."))
                    }
                } else {
                    canhypertrain(3, VAR_0x8004)
                    if(var(VAR_RESULT) == YES) {
                        removeitem(ITEM_BOTTLE_CAP)
                        hypertrain(STAT_SPEED, VAR_0x8004)
                        msgbox(format("Your POKéMON has been hypertrained in SPEED!"))
                    } else {
                        msgbox(format("SPEED stat of your POKéMON can no longer be trained."))
                    }
                }    
            } else {
                msgbox("You don't have any BOTTLE CAP.")
            }
        } else {
            checkitem(ITEM_GOLD_BOTTLE_CAP)
            if(var(VAR_RESULT) == YES){
                msgbox(format("Choose a POKéMON to fully hypertrain."))
                special(ChoosePartyMon)
                waitstate
                canhypertrain(0, VAR_0x8004)
                if(var(VAR_RESULT) == YES) {
                    removeitem(ITEM_GOLD_BOTTLE_CAP)
                    hypertrain(STAT_HP, VAR_0x8004)
                    hypertrain(STAT_ATK, VAR_0x8004)
                    hypertrain(STAT_DEF, VAR_0x8004)
                    hypertrain(STAT_SPATK, VAR_0x8004)
                    hypertrain(STAT_SPDEF, VAR_0x8004)
                    hypertrain(STAT_SPEED, VAR_0x8004)
                    msgbox(format("Your POKéMON has been fully hypertrained!"))
                } else {
                    canhypertrain(1, VAR_0x8004)
                    if(var(VAR_RESULT) == YES) {
                        removeitem(ITEM_GOLD_BOTTLE_CAP)
                        hypertrain(STAT_HP, VAR_0x8004)
                        hypertrain(STAT_ATK, VAR_0x8004)
                        hypertrain(STAT_DEF, VAR_0x8004)
                        hypertrain(STAT_SPATK, VAR_0x8004)
                        hypertrain(STAT_SPDEF, VAR_0x8004)
                        hypertrain(STAT_SPEED, VAR_0x8004)
                        msgbox(format("Your POKéMON has been fully hypertrained!"))
                    } else {
                        canhypertrain(2, VAR_0x8004)
                        if(var(VAR_RESULT) == YES) {
                            removeitem(ITEM_GOLD_BOTTLE_CAP)
                            hypertrain(STAT_HP, VAR_0x8004)
                            hypertrain(STAT_ATK, VAR_0x8004)
                            hypertrain(STAT_DEF, VAR_0x8004)
                            hypertrain(STAT_SPATK, VAR_0x8004)
                            hypertrain(STAT_SPDEF, VAR_0x8004)
                            hypertrain(STAT_SPEED, VAR_0x8004)
                            msgbox(format("Your POKéMON has been fully hypertrained!"))
                        } else {
                            canhypertrain(3, VAR_0x8004)
                            if(var(VAR_RESULT) == YES) {
                                removeitem(ITEM_GOLD_BOTTLE_CAP)
                                hypertrain(STAT_HP, VAR_0x8004)
                                hypertrain(STAT_ATK, VAR_0x8004)
                                hypertrain(STAT_DEF, VAR_0x8004)
                                hypertrain(STAT_SPATK, VAR_0x8004)
                                hypertrain(STAT_SPDEF, VAR_0x8004)
                                hypertrain(STAT_SPEED, VAR_0x8004)
                                msgbox(format("Your POKéMON has been fully hypertrained!"))
                            } else {
                                canhypertrain(4, VAR_0x8004)
                                if(var(VAR_RESULT) == YES) {
                                    removeitem(ITEM_GOLD_BOTTLE_CAP)
                                    hypertrain(STAT_HP, VAR_0x8004)
                                    hypertrain(STAT_ATK, VAR_0x8004)
                                    hypertrain(STAT_DEF, VAR_0x8004)
                                    hypertrain(STAT_SPATK, VAR_0x8004)
                                    hypertrain(STAT_SPDEF, VAR_0x8004)
                                    hypertrain(STAT_SPEED, VAR_0x8004)
                                    msgbox(format("Your POKéMON has been fully hypertrained!"))
                                } else {
                                    canhypertrain(5, VAR_0x8004)
                                    if(var(VAR_RESULT) == YES) {
                                        removeitem(ITEM_GOLD_BOTTLE_CAP)
                                        hypertrain(STAT_HP, VAR_0x8004)
                                        hypertrain(STAT_ATK, VAR_0x8004)
                                        hypertrain(STAT_DEF, VAR_0x8004)
                                        hypertrain(STAT_SPATK, VAR_0x8004)
                                        hypertrain(STAT_SPDEF, VAR_0x8004)
                                        hypertrain(STAT_SPEED, VAR_0x8004)
                                        msgbox(format("Your POKéMON has been fully hypertrained!"))
                                    } else {
                                        msgbox(format("Your POKéMON can no longer be trained."))
                                    }
                                }
                            }
                        }
                    }
                }
            } else {
                msgbox(format("You don't have any GOLD BOTTLE CAP."))
            }
        }
    } else {
        msgbox("Ok. I'll be here if you need me. Bye!")
    }
    releaseall
}

script BattleFrontier_ExchangeServiceCorner_EventScript_Candies {
	lock
    faceplayer
    msgbox(format("Psst...psst! Over here! Do you want some candies for free?"), MSGBOX_YESNO)
    if(var(VAR_RESULT) == YES){
		message(format("Take as many as you want."))
        multichoice(25, 0, MULTI_CANDIES, 0)
        closemessage
        if(var(VAR_RESULT) == 0){
			giveitem(ITEM_RARE_CANDY, 99)
			if(var(VAR_RESULT) == FALSE){
				goto(Common_EventScript_ShowBagIsFull)
			} else {
				msgbox(format("Come back for more whenever you want."))
			}
		} if(var(VAR_RESULT) == 1){
			giveitem(ITEM_EXP_CANDY_S, 99)
			if(var(VAR_RESULT) == FALSE){
				goto(Common_EventScript_ShowBagIsFull)
			} else {
				msgbox(format("Come back for more whenever you want."))
			}
		} if(var(VAR_RESULT) == 2){
			giveitem(ITEM_EXP_CANDY_M, 99)
			if(var(VAR_RESULT) == FALSE){
				goto(Common_EventScript_ShowBagIsFull)
			} else {
				msgbox(format("Come back for more whenever you want."))
			}
		} if(var(VAR_RESULT) == 3){
			giveitem(ITEM_EXP_CANDY_L, 99)
			if(var(VAR_RESULT) == FALSE){
				goto(Common_EventScript_ShowBagIsFull)
			} else {
				msgbox(format("Come back for more whenever you want."))
			}
		} if(var(VAR_RESULT) == 4){
			giveitem(ITEM_EXP_CANDY_XL, 99)
			if(var(VAR_RESULT) == FALSE){
				goto(Common_EventScript_ShowBagIsFull)
			} else {
				msgbox(format("Come back for more whenever you want."))
			}
		} else {
			giveitem(ITEM_EXP_CANDY_MAX, 99)
			if(var(VAR_RESULT) == FALSE){
				goto(Common_EventScript_ShowBagIsFull)
			} else {
				msgbox(format("Come back for more whenever you want."))
			}
		} 
	} else {
		msgbox("Ok, never mind.")
	}
	releaseall
}

script BattleFrontier_ExchangeServiceCorner_EventScript_MaxFriendship {
	lock
    faceplayer
    msgbox(format("I'm known as the best POKéMON masseuse in the world. I can make your POKéMON increase their friendship to the maximum with just one massage. And it's totally free! Do you want to try?"), MSGBOX_YESNO)
	if(var(VAR_RESULT) == YES){
		msgbox(format("Choose a POKéMON give it the masseuse."))
        special(ChoosePartyMon)
        waitstate
        canmaxfriendship(VAR_0x8004)
		if(var(VAR_RESULT) == YES) {
			maxfriendship(VAR_0x8004)
			msgbox(format("The friendship of this POKéMON is now maximized! Don't thank me."))
		} else {
			msgbox(format("The friendship of this POKéMON is already at its peak. My massages won't do much good."))
		}
	} else {
		msgbox(format("Come back when you want one of my amazing massages. See you!"))
	}
	releaseall
}